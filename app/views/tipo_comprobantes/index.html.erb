<h1>Listing tipo_comprobantes</h1>

<table>
  <tr>
    <th>Nombre</th>
  </tr>

<% @tipo_comprobantes.each do |tipo_comprobante| %>
  <tr>
    <td><%=h tipo_comprobante.nombre %></td>
    <td><%= link_to 'Show', tipo_comprobante %></td>
    <td><%= link_to 'Edit', edit_tipo_comprobante_path(tipo_comprobante) %></td>
    <td><%= link_to 'Destroy', tipo_comprobante, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tipo_comprobante', new_tipo_comprobante_path %>