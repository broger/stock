<h1>Editing comprobante</h1>

<% form_for(@comprobante) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :usuario_id %><br />
    <%= f.text_field :usuario_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @comprobante %> |
<%= link_to 'Back', comprobantes_path %>