<h1>Listing tipo_pagos</h1>

<table>
  <tr>
    <th>Nombre</th>
  </tr>

<% @tipo_pagos.each do |tipo_pago| %>
  <tr>
    <td><%=h tipo_pago.nombre %></td>
    <td><%= link_to 'Show', tipo_pago %></td>
    <td><%= link_to 'Edit', edit_tipo_pago_path(tipo_pago) %></td>
    <td><%= link_to 'Destroy', tipo_pago, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tipo_pago', new_tipo_pago_path %>