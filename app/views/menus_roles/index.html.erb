<h1>Listing menu_roles</h1>

<table>
  <tr>
    <th>Menu</th>
    <th>Rol</th>
  </tr>

<% @menus_roles.each do |menu_rol| %>
  <tr>
    <td><%=h menu_rol.menu_id %></td>
    <td><%=h menu_rol.rol_id %></td>
    <td><%= link_to 'Show', menu_rol %></td>
    <td><%= link_to 'Edit', edit_menus_rol_path(menu_rol) %></td>
    <td><%= link_to 'Destroy', menu_rol, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New menu_rol', new_menus_rol_path %>